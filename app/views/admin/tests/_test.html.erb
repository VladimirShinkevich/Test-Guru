<tr class='js-test-inline-edit'>
  <td class="align-middle"> 
    <%= content_tag :span, test.title, class: 'js-test-title', data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td class="align-middle"> <%= test_level(test) %> </td>
  <td class="align-middle"> <%= test.questions.size %> </td>
  <td class="align-middle"> <%= test.timer %> </td>
  <td class="align-middle"> <%= link_to t(".show"), admin_test_path(test) %> </td>
  <td class="align-middle"> 
    <%= link_to t(".edit"), edit_admin_test_path(test), class: "js-form-inline-link", data: { test_id: test.id } %> 
  </td>
  <td class="align-middle"> 
    <%= link_to t(".delete"), 
                admin_test_path(test), 
                method: :delete, 
                data: { confirm: t(".confirm") } %> 
  </td>
</tr>
